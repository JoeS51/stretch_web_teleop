<launch>
  
  <!-- REALSENSE D435i -->
  <include file="$(find stretch_core)/launch/d435i_low_resolution.launch"></include>
  <!-- -->

  <!-- GRIPPER CAMERA -->
  <include file="$(find stretch_web_interface_react)/launch/gripper_camera.launch"></include>
  <!-- -->

  <!-- NAVIGATION CAMERA -->
  <include file="$(find stretch_web_interface_react)/launch/navigation_camera.launch"></include>
  <!-- -->
  
  <!-- STRETCH DRIVER -->
  <param name="/stretch_driver/broadcast_odom_tf" type="bool" value="true"/>
  <include file="$(find stretch_core)/launch/stretch_driver.launch"></include>
  <!-- -->

  <!-- ROSBRIDGE -->
  <include file="$(find rosbridge_server)/launch/rosbridge_websocket.launch">
    <arg name="port" default="9090"/>
    <arg name="address" default="localhost" />
    <arg name="ssl" value="true" />
    <arg name="certfile" value="$(find stretch_web_interface_react)/certificates/slinky.hcrlab.cs.washington.edu+6.pem" />
    <arg name="keyfile" value="$(find stretch_web_interface_react)/certificates/slinky.hcrlab.cs.washington.edu+6-key.pem" />
    <arg name="authenticate" default="false" />
  </include>

  <node name="tf2_web_republisher" pkg="tf2_web_republisher" type="tf2_web_republisher" respawn="false" output="screen"/>

  <arg name="params" value="$(find stretch_web_interface_react)/nodes/configure_video_streams_params.yaml"/>
  <node name="configure_video_streams" pkg="stretch_web_interface_react" type="configure_video_streams" args="$(arg params)" output="screen"/>
  <!-- -->
  
</launch>